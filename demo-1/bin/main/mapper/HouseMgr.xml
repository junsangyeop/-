<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.HouseMgrDao">
    <select id="selectHouseMgrUserCheck" parameterType="com.example.demo.bean.HouseMgrUserBean" resultType="Boolean">
    	SELECT COUNT(*) 
    	FROM LRMGR_USER
    	WHERE EMAIL = #{email}
    		AND PASSWD = #{passwd}
    </select>
</mapper>
