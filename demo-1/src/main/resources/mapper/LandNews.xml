<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.LandNewsDao">
    <select id="selectNewsList" parameterType="string" resultType="com.example.demo.bean.LandNewsBean">
    	SELECT NEWS_DATE, TITLE, SUB_TITLE, CONTENT FROM LAND_NEWS LIMIT 10
    </select>
</mapper>
